{
  "expo": {
    "name": "Thought Bubble",
    "slug": "thought-bubble",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconTemp.png",
    "scheme": "com.sephclt.thoughtbubble",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sephclt.thoughtbubble"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/iconTemp.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/images/onboarding_images/ob_page_1.png",
            "./assets/images/onboarding_images/ob_page_2.png",
            "./assets/images/onboarding_images/ob_page_3.png",
            "./assets/images/onboarding_images/ob_page_4.png"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "68cc7b87-f716-4f55-9715-90cafa130eeb"
      }
    },
    "owner": "sephclt"
  }
}
